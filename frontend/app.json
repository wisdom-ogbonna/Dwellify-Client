{
  "expo": {
    "name": "Dwellify",
    "slug": "dwellify",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/dwellify-black.png",
    "scheme": "dwellify",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/dwellify-black.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/dwellify-black.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/images/dwellify-black.png"
    },
    "plugins": [
      "expo-video"
    ]
  }
}
